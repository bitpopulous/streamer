{
    "name": "populous/streamer",
    "type": "library",
    "description": "packages for websocket streamer server in populous",
    "keywords": ["websocket","wss","populous","ppt", "streamer"],
    "homepage": "https://populous.world",
    "license": "MIT",
    "authors": [
        {
            "name": "Remi Besnard",
            "email": "bluesven869@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "arthurkushman/php-wss": "^1.6",
        "php": ">=7.1",
        "ext-pcntl": "*"
    },
    "autoload": {
        "psr-4": {
            "PopulousWSS\\": "src",
            "PopulousWSSTEST\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PopulousWSS\\": "src",
            "PopulousWSSTEST\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "monolog/monolog": "^1.24"
    }
}